---
import Layout from "../layouts/Layout.astro";
import Countdown from "../components/Countdown.astro";
import HeroImage from "../components/HeroImage.astro";
import usImage from "../assets/images/home/us.webp";
import Agenda from "../components/Agenda.astro";
import { WEDDING_DATE } from "../constants";
---

<Layout title="Ash + Anna" hideHeading>
  <Countdown countdownDate={WEDDING_DATE} />
  <HeroImage altText="A picture of us" image={usImage} width={600} />
  <Agenda />
</Layout>
